{
  "extends": "airbnb-base",
  "rules": {
    "no-console": 0,
    "radix": 0,
    "semi": 0,
    "comma-spacing": 0,
    "arrow-spacing": 0,
    "space-infix-ops": 0,
    "array-callback-return": 0,
    "one-var": 0,
    "one-var-declaration-per-line": 0,
    "no-plusplus": 0,
    "indent": 1,
    "no-trailing-spaces": 1,
    "padded-blocks": 1,
    "no-use-before-define": 1,
    "spaced-comment": 1,
    "keyword-spacing": 1,
    "no-unused-vars": 1,
    "no-param-reassign": 1,
    "consistent-return": 1,
    "comma-dangle": ["error", "never"]
    }
}
